<?xml version="1.0" encoding="UTF-8"?>

<root>
  <component name="com.trdos.cores.Ma" type="Receiver">
    <manifest>
      <intent_filter action="android.provider.Telephony.SMS_RECEIVED"/>
    </manifest>
    <sender destination="com.trdos.cores.Sb, interICC_, " action="" flags="FLAG_ACTIVITY_NEW_TASK "/>
    <receive action="notEmpty"/>
  </component>
  <component name="com.trdos.cores.Sb" type="Service" exported="true">
    <sender destination="com.trdos.cores.Sb" flags="FLAG_ACTIVITY_NEW_TASK "/>
  </component>
  <component name="com.trdos.cores.Dc" type="Service" exported="true">
    <sender destination="com.trdos.cores.Dc" flags="FLAG_ACTIVITY_NEW_TASK "/>
  </component>
  <component name="com.trdos.cores.MainActivity" type="Activity">
    <manifest>
      <intent_filter action="android.intent.action.MAIN" category="android.intent.category.LAUNCHER"/>
    </manifest>
    <sender destination="com.trdos.cores.Dc, com.trdos.cores.Sb, com.trdos.cores.Db" flags="FLAG_ACTIVITY_NEW_TASK "/>
  </component>
  <component name="com.trdos.cores.Db" type="Receiver">
    <manifest>
      <intent_filter action="android.intent.action.BOOT_COMPLETED, android.intent.action.SCREEN_ON" category="android.intent.category.HOME"/>
    </manifest>
    <sender destination="com.trdos.cores.Sb"/>
  </component>
  <component name="com.trdos.cores.Da" type="Receiver" permission="android.permission.BIND_DEVICE_ADMIN">
    <manifest>
      <intent_filter action="android.app.action.DEVICE_ADMIN_ENABLED"/>
      <intent_filter action="android.app.action.ACTION_DEVICE_ADMIN_DISABLED, android.app.action.DEVICE_ADMIN_ENABLED, android.app.action.ACTION_DEVICE_ADMIN_DISABLE_REQUESTED"/>
    </manifest>
  </component>
</root>
